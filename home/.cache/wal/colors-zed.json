{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#12070790",
                "editor.background": "#12070790",
                "editor.foreground": "#c3c1c1",
                "text": "#c3c1c1",
                "text.muted": "#c3c1c170",
                "text.ignored": "#c3c1c140",
                "text.placeholder": "#c3c1c150",
                "ignored": "#c3c1c130",
                "element.hover": "#c3c1c130",
                "ghost_element.hover": "#A35A5B30",
                "ghost_element.selected": "#A35A5B30",
                "ghost_element.active": "#A35A5B60",
                "border": "#120707",
                "editor.highlighted_line.background": "#A35A5B10",
                "editor.active_line.background": "#A35A5B10",
                "panel.background": "#12070790",
                "title_bar.background": "#12070790",
                "title_bar.inactive_background": "#12070790",
                "status_bar.background": "#12070790",
                "drop_target.background": "#12070790",
                "elevated_surface.background": "#120707",
                "toolbar.background": "#12070790",
                "tab_bar.background": "#12070790",
                "tab.inactive_background": "#12070790",
                "tab.active_background": "#A35A5B30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#120707",
                "scrollbar.thumb.background": "#A35A5B",
                "editor.gutter.background": "#12070790",
                "terminal.background": "#12070710",
                "terminal.foreground": "#c3c1c1",
                "terminal.dim_foreground": "#c3c1c1",
                "terminal.bright_foreground": "#c3c1c1",
                "terminal.ansi.black": "#120707",
                "terminal.ansi.red": "#A35A5B",
                "terminal.ansi.green": "#D0745D",
                "terminal.ansi.yellow": "#E4883F",
                "terminal.ansi.blue": "#EB9A69",
                "terminal.ansi.magenta": "#FBC876",
                "terminal.ansi.cyan": "#F7B185",
                "terminal.ansi.white": "#c3c1c1",
                "terminal.ansi.bright_black": "#6a5656",
                "terminal.ansi.bright_red": "#A35A5B",
                "terminal.ansi.bright_green": "#D0745D",
                "terminal.ansi.bright_yellow": "#E4883F",
                "terminal.ansi.bright_blue": "#EB9A69",
                "terminal.ansi.bright_magenta": "#FBC876",
                "terminal.ansi.bright_cyan": "#F7B185",
                "terminal.ansi.bright_white": "#c3c1c1",
                "modified": "#A35A5B",
                "syntax": {
                    "attribute": {
                        "color": "#A35A5B"
                    },
                    "boolean": {
                      "color": "#D0745D"
                    },
                    "comment": {
                      "color": "#6a5656"
                    },
                    "comment.doc": {
                      "color": "#6a5656"
                    },
                    "constant": {
                      "color": "#D0745D"
                    },
                    "function": {
                      "color": "#F7B185"
                    },
                    "keyword": {
                      "color": "#EB9A69"
                    },
                    "number": {
                      "color": "#FBC876"
                    },
                    "operator": {
                      "color": "#EB9A69"
                    },
                    "string": {
                      "color": "#A35A5B"
                    },
                    "variable": {
                      "color": "#D0745D"
                    }
                },
                "players": [
                  {
                    "cursor": "#A35A5B",
                    "background": "#120707",
                    "selection": "#A35A5B30"
                  }
                ]
            }
        }
    ]
}
